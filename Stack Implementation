class ArrayStack {

    private int[] stack;
    private int top;
    private final int capacity = 100;

    public ArrayStack() {
        stack = new int[capacity];
        top = -1;
    }

    public void push(int x) {
        if (top == capacity - 1) {
            System.out.println("Stack is full!");
            return;
        }
        top++;
        stack[top] = x;
    }

    public int pop() {
        if (isEmpty()) {
            System.out.println("Stack is empty!");
            return -1; 
        }
        int data = stack[top];
        top--;
        return data;
    }

    public int top() {
        if (isEmpty()) {
            System.out.println("Stack is empty!");
            return -1; 
        }
        return stack[top];
    }

    public boolean isEmpty() {
        return top == -1;
    }
}
